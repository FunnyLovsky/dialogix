{
    "name": "dialogix",
    "version": "1.0.0",
    "description": "A powerful tool for generating prompts tailored for ChatGPT, harnessing the latest in AI technology to enhance conversational experiences.",
    "main": "index.js",
    "scripts": {
        "dev": "webpack serve --env mode=development",
        "build:dev": "webpack --env mode=development",
        "build:prod": "webpack --env mode=production",
        "typecheck": "tsc",
        "format": "prettier --write ."
    },
    "keywords": [
        "chat",
        "GPT",
        "prompt",
        "AI",
        "conversation",
        "generation"
    ],
    "author": "Funny_lovsky",
    "license": "ISC",
    "devDependencies": {
        "@svgr/webpack": "^8.1.0",
        "@types/babel__core": "^7.20.3",
        "@types/node": "^20.11.16",
        "@types/webpack": "^5.28.3",
        "@types/webpack-bundle-analyzer": "^4.6.1",
        "@types/webpack-dev-server": "^4.7.2",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "fork-ts-checker-webpack-plugin": "^9.0.0",
        "html-webpack-plugin": "^5.5.3",
        "husky": "^4.3.8",
        "lint-staged": "^15.2.2",
        "mini-css-extract-plugin": "^2.8.0",
        "prettier": "^3.2.5",
        "sass": "^1.69.0",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.5.0",
        "ts-node": "^10.9.1",
        "tsc": "^2.0.4",
        "typescript": "^5.3.3",
        "webpack": "^5.88.2",
        "webpack-bundle-analyzer": "^4.9.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "dependencies": {
        "mammoth": "^1.6.0",
        "path-browserify": "^1.0.1",
        "util": "^0.12.5"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
